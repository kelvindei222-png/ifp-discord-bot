{
  "name": "ifp-core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "postbuild": "copy package*.json dist\\ 2>nul || echo 'Package files copied'",
    "prod": "npm run build && npm start",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist",
    "deploy-commands": "ts-node src/deploy-commands.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.6",
    "canvas": "^3.1.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.2.0",
    "node-fetch": "^2.7.0",
    "opusscript": "^0.0.8",
    "prism-media": "^1.3.5",
    "spotify-web-api-node": "^5.0.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "rimraf": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=8.0.0"
  }
}
